[beets]
library: ~/Music/musiclibrary.blb
directory: ~/Music
path_format: $artist/$album/$track - $title
import_copy: yes
import_write: yes
import_resume: ask
import_art: yes
import_quiet_fallback: skip
import_timid: no
import_log: beetslog.txt
ignore: .AppleDouble ._* *~ .DS_Store
art_filename: cover
threaded: yes
color: yes

[paths]
default: $albumartist/$album/$track - $title